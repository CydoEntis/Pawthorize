{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "System": "Warning",
      "Pawthorize": "Debug",
      "Pawthorize.AspNetCore.Handlers": "Information",
      "Pawthorize.AspNetCore.Services": "Information",
      "Pawthorize.Jwt.Services": "Information",
      "Pawthorize.Security.Services": "Debug"
    },
    "Console": {
      "IncludeScopes": true,
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss "
    }
  },
  "AllowedHosts": "*"
}
